ReplayRetryTime = 30.0
ProcessesToKillDuringTest = (?!x)x
ShowSettingsAfterTest = true
ForceToSequenceLength = true
OutputDir = .\\output\\
UnattendedTests = false
TestGenerator = random
StartupTime = 8.0
ActionDuration = 0.1
NonReactingUIThreshold = 100
