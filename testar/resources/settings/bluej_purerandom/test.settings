TempDir = .\\output\\temp\\
MaxTime = 3.1536E7
StopGenerationOnFault = true
ExecuteActions = true
DrawWidgetTree = false
UseRecordedActionDurationAndWaitTimeDuringReplay = true
ForceForeground = true
AlgorithmFormsFilling = false
FaultThreshold = 0.1
DrawWidgetInfo = true
MaxReward = 9999999.0
LogLevel = 1
VisualizeSelectedAction = false
Delete = 
