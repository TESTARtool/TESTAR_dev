                  <Insets left="8.0" />
               </padding>
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/attach.png" />
                     </image>
                  </ImageView>
               </graphic>
               <GridPane.margin>
                  <Insets right="8.0" />
               </GridPane.margin>
            </JFXButton>
            <HBox alignment="CENTER_LEFT" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="1">
               <children>
                  <Label id="labelTmpDir" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Not selected" textFill="WHITE" HBox.hgrow="ALWAYS">
